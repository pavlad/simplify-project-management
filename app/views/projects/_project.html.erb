<tr>
  <td data-label="Status" class="mdl-data-table__cell--non-numeric project-status""><a class="ui label high-priority-issue">High Priority Issue</a></td>
  <td class="project-name"><%= project.name %></td>
  <td class="project-client"><%= project.client ? project.client.name : "-" %> </td>
  <td class="project-budget"><%= project.man_days ? project.man_days : "-" %></td>
  <td class="project-progress"><%= project.tasks.size == 0 ? "-" : "#{project.completion_percentage.to_i}%" %></td>
  <td class="project-project-lead">
  <% if project.project_manager %>
    <div class="consultant-avatars"><span data-title="<%= project.project_manager.full_name if project.project_manager %>" data-content=<%= project.project_manager.job_title if project.project_manager.job_title %> class="project-manager-popup-trigger"><img class="ui mini circular image" src="./img/lukas.png"></span></div>
  <% end %>
  </td>
  <td><%= link_to 'details', project_path(project) %></td>
</tr>
