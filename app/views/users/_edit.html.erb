<%= simple_form_for [@user] do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :job_title %>
  <%= f.input :email %>
  <%= f.input :mobile_phone %>
  <%= f.input :phone %>
  <%= f.input :description, input_html: { rows: 2 }%>
  <%= f.input :is_owner %>
  <%= f.button :submit %>
<% end %>
